#include <iostream>

using namespace std;

int main() {
    int a[5]={1,2,3,1,4};
    int b[5]={0};
    for(int i=0;i<5;i++){
        cout << a[i] <<"\t";
    }
    cout<< endl;
    for(int i=0;i<5;i++){
        if(b[a[i]]>0){
            cout << a[i] << endl;
        }
        b[a[i]]++;
    }
    return 0;
}