#include <iostream>

struct node{
    int data;
    struct node *next;
};

#creating a node
int main() {
    struct node *head = NULL;
    head = (struct node *)malloc(sizeof(struct node));
    head->data = 3;
    head->next = NULL;
    std::cout << head->data;

    return 0;
}

#printing the elements of a linked list
void printLinkedList(struct node *head){
    while(head!=NULL){
        std::cout <<head->data <<"\n";
        head = head->next;
    }
}
